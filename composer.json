{
    "name": "azman/image_tag_analysis",
    "description": "Drupal 10/11 custom project with private module integration.",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:azmunkz/image_tag_analysis.git"
        }
    ],
    "require": {
        "drupal/core": "^10 || ^11",
        "azman/image_tag_analysis": "1.2.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
